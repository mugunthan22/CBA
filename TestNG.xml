<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Test Suite">
	<listeners>
		<listener class-name="report.TestListener" />
	</listeners>
	<parameter name="browserType" value="chrome" />
	<test name="Pre-Condition" >
		<classes>
			<class name="Precondition" />
		</classes>
	</test>

	<test name="API Tests" >
		<classes>
			<class name="apitest.Testcase" />
		
		</classes>
	</test>
	<test name="UI Tests" >
		<classes>
			<class name="uitest.TestCase" />
		
		</classes>
	</test>
	
</suite>